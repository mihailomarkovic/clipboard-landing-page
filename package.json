  "bugs": {
    "url": "https://github.com/mihailomarkovic/clipboard-landing-page/issues"
  },
  "homepage": "https://github.com/mihailomarkovic/clipboard-landing-page#readme",
  "description": "",
  "devDependencies": {
    "node-sass": "^9.0.0"
  }
}
